{
  "name": "clean-architecture-node-api-starter",
  "version": "1.0.0",
  "description": "Clean Architecture API starter kit",
  "keywords": [
    "node",
    "clean-architecture",
    "api"
  ],
  "scripts": {
    "http:dev": "concurrently --raw \"npm run build:dev\" \"npm run copy:sql\" \"npm run run:http:dev\" ",
    "cli:prod": "concurrently --raw \"npm run build\"  \"npm run copy:sql\" \"npm run run:cli:prod\" ",
    "copy:sql": "rsync -a --prune-empty-dirs --exclude 'dist/*' --include '*/' --include '*.sql' --exclude '*' ./ ./dist/",
    "build": "tsc",
    "build:dev": "tsc --watch",
    "run:http:dev": "env NODE_ENV=development nodemon -q --inspect dist/src/infrastructure/http/http.js",
    "run:http:stage": "env NODE_ENV=staging node dist/src/infrastructure/http/http.js",
    "run:http:prod": "env NODE_ENV=production node dist/src/infrastructure/http/http.js",
    "run:cli:prod": "env NODE_ENV=production node --inspect dist/src/infrastructure/cli/cli.js",
    "run:http:resetdb": "env NODE_ENV=staging node dist/src/infrastructure/persistence/mysql/.js"
  },
  "author": "Antonio DÃ­az",
  "license": "ISC",
  "dependencies": {
    "@antoniodcorrea/components": "^2.0.8",
    "@antoniodcorrea/utils": "^1.0.279",
    "@antoniodcorrea/utils-backend": "^1.0.278",
    "@types/express": "^4.17.6",
    "@types/jimp": "^0.2.28",
    "@types/jsdom": "^21.1.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mmmagic": "^0.4.30",
    "@types/multer": "^1.4.5",
    "@types/node": "^20.11.19",
    "@types/nodemailer": "^6.4.0",
    "axios": "^1.6.7",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "jimp": "^0.22.10",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^9.0.2",
    "languagedetect": "^2.0.0",
    "mkdirp": "^3.0.1",
    "mmmagic": "^0.5.3",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.1",
    "nodemailer": "^6.5.0",
    "nodemon": "^3.0.3",
    "prompts": "^2.3.2",
    "psl": "^1.8.0",
    "qs": "^6.9.6",
    "tslib": "^2.1.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@types/xml": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "_moduleAliases": {
    "@infrastructure": "dist/src/infrastructure",
    "@domain": "dist/src/domain",
    "@adapter": "dist/src/adapter",
    "@shared": "dist/src/shared",
    "@tools": "dist/src/tools",
    "@root": "dist"
  }
}
